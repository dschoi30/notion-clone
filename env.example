# ---- Database ----
POSTGRES_DB=notion_clone
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DB_PORT=5432

# JDBC URL used by Spring Boot
DB_URL=jdbc:postgresql://db:5432/notion_clone
DB_USERNAME=postgres
DB_PASSWORD=postgres

# ---- Auth / JWT ----
JWT_SECRET=your-super-secret-jwt-key-that-is-at-least-32-bytes-long-for-security
JWT_EXPIRATION=604800000

# ---- Google OAuth ----
GOOGLE_CLIENT_ID=

# ---- Cloudinary ----
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=
CLOUDINARY_UPLOAD_PRESET=

# ---- Frontend / Backend Ports ----
BACKEND_PORT=8080
FRONTEND_PORT=5173

# ---- Frontend runtime config ----
# Vite dev proxy target for /api and /ws
VITE_BACKEND_ORIGIN=http://backend:8080
# axios baseURL fallback (services/api.js)
VITE_API_BASE_URL=/api


